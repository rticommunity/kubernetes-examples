<dds>
    <qos_library name="RWT_Demo">
    <qos_profile name="RWT_Profile">
        <participant_qos>
            <transport_builtin>
                <mask>UDPv4_WAN</mask>
            </transport_builtin>
            <discovery>
                <initial_peers>
                    <element>
                        rtps@udpv4_wan://$(CDS_PUBLIC_IP):$(CDS_PUBLIC_PORT)
                    </element>
                </initial_peers>
            </discovery>
        </participant_qos>
    </qos_profile>
    </qos_library>
</dds>

